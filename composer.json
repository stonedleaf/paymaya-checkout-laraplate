{
    "name": "stonedleaf/paymaya-checkout-laraplate",
    "keywords": ["laravel", "paymaya", "checkout"],
    "description": "Basic laravel boiler plate for consuming Paymaya checkout APIs",
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "illuminate/database": "^7.9|^8.0",
        "illuminate/http": "^7.9|^8.0",
        "illuminate/routing": "^7.9|^8.0",
        "nesbot/carbon": "^2.0",
        "moneyphp/money": "^3.2|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Stonedleaf\\PaymayaLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stonedleaf\\PaymayaLaravel\\PaymayaServiceProvider"
            ]
        }
    }
}
